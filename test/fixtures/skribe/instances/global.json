{
  "$id": "global",
  "$class": "global",
  "site": {
    "title": "Skribe Demo",
    "url": "https://example.com",
    "description": "A static site generator powered by Struktur"
  },
  "stylesheets": [
    "style.css",
    "custom.css"
  ],
  "$render": [
    {
      "template": "index.html",
      "output": "/index.html"
    },
    {
      "template": "style.css",
      "output": "/style.css"
    },
    {
      "template": "custom.css",
      "output": "/custom.css"
    }
  ]
}
