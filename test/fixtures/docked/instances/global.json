{
  "$id": "global",
  "$class": "docked_stack",
  "stack_name": "Docker Docked Stack",
  "description": "Complete Docker container infrastructure with relationship-driven configuration. Automatic service integration, credentials management, and beautiful visualization.",
  "$render": [
    {
      "template": "viewer.html",
      "output": "/index.html"
    },
    {
      "template": "nginx-index.html",
      "output": "/nginx/index.html"
    },
    {
      "template": "docker-compose.yml",
      "output": "/docker-compose.yml"
    },
    {
      "template": "dot.env.example",
      "output": "/.env.example"
    },
    {
      "template": "grafana-datasources.yml",
      "output": "/grafana-datasources.yml"
    }
  ]
}
