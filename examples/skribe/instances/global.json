{
  "$id": "global",
  "$class": "global",
  "description": "Skribe site configuration and build manifest",
  "site": {
    "title": "Skribe Demo",
    "url": "https://example.com",
    "description": "A static site generator powered by Struktur"
  },
  "stylesheets": [
    "css/style.css",
    "css/custom.css"
  ],
  "$render": [
    {
      "template": "index.html",
      "output": "/index.html"
    },
    {
      "template": "css/style.css",
      "output": "/css/style.css"
    },
    {
      "template": "css/custom.css",
      "output": "/css/custom.css"
    }
  ]
}
