{
  "id": "global",
  "class": "docked_stack",
  "stack_name": "Docked Full-Stack Application",
  "description": "Complete full-stack web application with PostgreSQL database, Redis cache, Node.js backend API, and Nginx serving frontend and reverse proxy. Multi-environment support (dev/staging/prod) with health checks, resource limits, and proper dependency management.",
  "build": [
    {
      "viewer.html": "/index.html"
    },
    {
      "frontend.html": "/frontend/index.html"
    },
    {
      "docker-compose.yml": "/docker-compose.yml"
    },
    {
      "dot.env.example": "/.env.example"
    },
    {
      ".env.development": "/.env.development"
    },
    {
      ".env.staging": "/.env.staging"
    },
    {
      ".env.production": "/.env.production"
    },
    {
      "nginx.conf": "/nginx.conf"
    },
    {
      "api/server.js": "/api/server.js"
    },
    {
      "api/package.json": "/api/package.json"
    }
  ]
}
