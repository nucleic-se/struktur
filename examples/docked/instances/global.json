{
  "id": "global",
  "class": "docked_stack",
  "stack_name": "Docked Production Stack",
  "description": "Production-ready Docker infrastructure with PostgreSQL, Redis, Node.js API, Nginx reverse proxy, and monitoring. Multi-environment support (dev/staging/prod) with health checks, resource limits, and proper dependency management.",
  "build": [
    {
      "viewer.html": "/index.html"
    },
    {
      "nginx-index.html": "/nginx/index.html"
    },
    {
      "docker-compose.yml": "/docker-compose.yml"
    },
    {
      "dot.env.example": "/.env.example"
    },
    {
      "grafana-datasources.yml": "/grafana-datasources.yml"
    },
    {
      ".env.development": "/.env.development"
    },
    {
      ".env.staging": "/.env.staging"
    },
    {
      ".env.production": "/.env.production"
    },
    {
      "nginx.conf": "/nginx.conf"
    },
    {
      "api/server.js": "/api/server.js"
    },
    {
      "api/package.json": "/api/package.json"
    }
  ]
}
